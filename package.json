{
  "name": "aws-setup",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "bootstrap": "source .env && cdk bootstrap aws://$ACCOUNT_ID/eu-west-1 && cdk bootstrap aws://$ACCOUNT_ID/us-east-1",
    "build": "tsc",
    "ci": "pnpm run '/^(build|lint|test)$/'",
    "lint": "eslint",
    "test": "vitest run",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.980.0",
    "@aws-sdk/client-identitystore": "^3.980.0",
    "@aws-sdk/client-sns": "^3.980.0",
    "@aws-sdk/client-sso-admin": "^3.980.0",
    "aws-cdk-lib": "^2.236.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@types/aws-lambda": "^8.10.160",
    "@types/node": "^25.1.0",
    "aws-sdk-client-mock": "^4.1.0",
    "constructs": "^10.4.5",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
