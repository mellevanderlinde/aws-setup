{
  "name": "aws-setup",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "bootstrap": "source .env && cdk bootstrap aws://$ACCOUNT_ID/eu-west-1 && cdk bootstrap aws://$ACCOUNT_ID/us-east-1",
    "build": "tsc",
    "ci": "npm run lint && npm run build && npm run test",
    "lint": "eslint",
    "test": "vitest run",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.965.0",
    "@aws-sdk/client-identitystore": "^3.965.0",
    "@aws-sdk/client-sns": "^3.965.0",
    "@aws-sdk/client-sso-admin": "^3.965.0",
    "aws-cdk-lib": "^2.234.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^25.0.3",
    "aws-sdk-client-mock": "^4.1.0",
    "constructs": "^10.4.4",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
