{
  "name": "aws-setup",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "bootstrap": "source .env && cdk bootstrap aws://$ACCOUNT_ID/eu-west-1 && cdk bootstrap aws://$ACCOUNT_ID/us-east-1",
    "build": "tsc",
    "ci": "npm run lint && npm run build && npm run test",
    "lint": "eslint",
    "test": "vitest run",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.879.0",
    "@aws-sdk/client-identitystore": "^3.879.0",
    "@aws-sdk/client-sns": "^3.879.0",
    "@aws-sdk/client-sso-admin": "^3.879.0",
    "aws-cdk-lib": "^2.213.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.3.0",
    "aws-sdk-client-mock": "^4.1.0",
    "cdk-nag": "^2.37.12",
    "constructs": "^10.4.2",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
